{
	"$schema": "https://schemas.wp.org/wp/6.1/theme.json",
	"version": 2,
	"customTemplates": [
    {
			"name": "page-full-width-template",
			"title": "page full width template",
			"postTypes": ["page"]
	},
	{
			"name": "page-template-blank",
			"title": "page template blank",
			"postTypes": [ "page" ]
	},
	{
			"name": "page-with-header",
			"title": "Page With Header",
			"postTypes": [ "page" ]
	},
	{
			"name": "page-with-left-sidebar",
			"title": "page with left sidebar",
			"postTypes": [ "page" ]
	}
  ],
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"color": {
			"duotone": [
        {
          "colors": [
            "#EEEEEE",
            "#111"
          ],
          "name": "Bg prim & Prim Color",
          "slug": "bg-prim-and-prim-color"
        },
        {
          "colors": [
            "#FFFFFF",
            "#838181"
          ],
          "name": "Bg-sec and Secondary Color",
          "slug": "bg-sec-and-sec-color"
        },
        {
          "colors": [
            "#111",
            "#111"
          ],
          "name": "Accent & Prim-color",
          "slug": "accent-and-prim-color"
        },
        {
          "colors": [
            "#111",
            "#838181"
          ],
          "name": "Accent & Secondary Color",
          "slug": "accent-and-sec-color"
        }
      ],
			"gradients": [
				{
					"gradient": "linear-gradient(to right, #0f2027, #203a43, #2c5364)",
					"name": "MoonlitAsteroid secondary to bg-prim",
					"slug": "moonlitasteroid-secondary-to-bg-prim"
				},
				{
					"gradient": "linear-gradient(to right, #ff9966, #ff5e62)",
					"name": "Orange Coral to bg-sec",
					"slug": "orange-coral-to-bg-sec"
				},
				{
					"gradient": "linear-gradient(to right, #0575e6, #021b79)",
					"name": "VeryBlue toprim-color",
					"slug": "veryblue-tertiary-to-prim-color"
				},
				{
					"gradient": "linear-gradient(to right, #dbe6f6, #c5796d)",
					"name": "Jaipur primary to sec-color",
					"slug": "jaipur-primary-to-sec-color"
				},
				{
					"gradient": "linear-gradient(to right, #283048, #859398)",
					"name": "Titanium to accent",
					"slug": "titanium-secondary-to-accent"
				},
				{
					"gradient": "linear-gradient(to right, #0052d4, #4364f7, #6fb1fc)",
					"name": "Bluelagoo background to btnclr",
					"slug": "bluelagoo-background-to-btnclr"
				},
				{
					"gradient": "linear-gradient(to right, #02aab0, #00cdac)",
					"name": "Green Beach tertiary to btnclr",
					"slug": "greenbeach-to-btnclr"
				},
				{
					"gradient": "linear-gradient(to right, #1d976c, #93f9b9)",
					"name": "Mojito background to btnclr",
					"slug": "mojito-background-to-btnclr"
				}
			],
			 "palette": [
        {
               "color": "#FFF",
               "name": "Bg Primary",
               "slug": "bg-prim"
            },
            {
              "color": "#F4F5F6",
              "name": "Bg secondary",
              "slug": "bg-sec"
            },
            {
              "name": "Primary Color",
              "color": "#111",
              "slug": "prim-color"
            },
            {
              "name": "Secondary Color",
              "color": "#838181",
              "slug": "sec-color"
            },
            {
              "color": "#111",
              "name": "Accent",
              "slug": "accent"
            },
            {
              "color": "#FFF",
              "name": "Button Color",
              "slug": "btnclr"
            }
      ]
		},
		"custom": {
			"spacing": {
				"baseline": "16px",
				"gap": {
					"horizontal": "clamp( calc( 1.5 * var( --wp--custom--spacing--baseline ) ), 2.222vw, calc( 2 * var( --wp--custom--spacing--baseline ) ) )",
					"vertical": "clamp( calc( 1.5 * var( --wp--custom--spacing--baseline ) ), 3.333vw, calc( 3 * var( --wp--custom--spacing--baseline ) ) )"
				},
				"gutter": "clamp( calc( 1.5 * var( --wp--custom--spacing--baseline ) ), 3.333vw, calc( 3 * var( --wp--custom--spacing--baseline ) ) )",
				"outer": "var( --wp--custom--spacing--gutter )",
				"small": "min(24px, 6.4vw)"
			}
		},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1280px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"customFontSize": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Poppins",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/poppins/Poppins-Regular.ttf"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "Poppins",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/poppins/Poppins-Bold.ttf"
							]
						}
					],
					"fontFamily": "Poppins, serif",
					"name": "Poppins",
					"slug": "poppins"
				},
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Inter",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "200 900",
							"src": [
								"file:./assets/fonts/Inter/Inter-VariableFont.ttf"
							]
						}
					],
					"fontFamily": "Inter, serif",
					"name": "Inter",
					"slug": "inter"
				}
			],
			"fontSizes": [
				{
					"name": "Tiny",
					"size": "13px",
					"slug": "tiny"
				},
				{
					"name": "Extra Small",
					"size": "14px",
					"slug": "extra-small"
				},
				{
					"name": "Small",
					"size": "15px",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "16px",
					"slug": "medium"
				},
				{
					"name": "Upper Heading",
					"size": "18px",
					"slug": "upper-heading"
				},
				{
					"name": "Normal",
					"size": "clamp(18px,1.6vw,20px)",
					"slug": "normal"
				},
				{
					"name": "Content Heading",
					"size": "clamp(22px,4.2vw,24px)",
					"slug": "content-heading"
				},
				{
					"name": "Large",
					"size": "clamp(28px,4.2vw,32px)",
					"slug": "large"
				},
				{
					"name": "Extra large (Recent Post Title)",
					"size": "clamp(32px,6vw,36px)",
					"slug": "extra-large"
				},
				{
					"name": "Wide",
					"size": "clamp(27px,5vw,45px)",
					"slug": "wide"
				},
				{
					"name": "Vast",
					"size": "clamp(35px,5vw,55px)",
					"slug": "vast"
				},
				{
					"name": "Broad",
					"size": "clamp(36px,6vw,70px)",
					"slug": "broad"
				},
				{
					"name": "Huge (Display)",
					"size": "clamp(36px,5.2vw,80px)",
					"slug": "huge"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/columns": {
				"border": {
					"radius": "10px"
				  }
			},
			"core/navigation": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--upper-heading)",
					"fontWeight": "600",
					"lineHeight": "normal"
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/button": {
		        "border": {
		          "radius": "4px"
		        },
				"color": {
					"text": "#ffffff",
					"background": "var(--wp--preset--color--accent)"
				}
		   }
		},
		"color": {
			"background": "var(--wp--preset--color--bg-prim)",
			"text": "var(--wp--preset--color--prim-color)"
		},
		 "spacing": {
      "padding": {
				"right": "18px",
				"left": "18px"
			}
    },
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "normal"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "var(--wp--preset--font-size--upper-heading)",
					"lineHeight": "normal"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "var(--wp--preset--font-size--content-heading)",
					"lineHeight": "normal"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "var(--wp--preset--font-size--normal)",
					"lineHeight": "normal"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "normal"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "var(--wp--preset--font-size--extra-small)",
					"lineHeight": "normal"
				}
			},
			"link": {
				":active": {
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"typography": {
						"textDecoration": "none"
					}
				},
				":hover": {
					"typography": {
						"textDecoration": "none"
					}
				},
				"color": {
          		"text": "var(--wp--preset--color--prim-color)"
        },
        		"typography": {
          		"textDecoration": "none"
        }
				
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--poppins)",
			"fontSize": "var(--wp--preset--font-size--upper-heading)",
			"lineHeight": "normal"
		}
	}
}