{
	"$schema": "https://schemas.wp.org/wp/6.1/theme.json",
	"version": 2,
	"customTemplates": [
    {
			"name": "page-left-sidebar",
			"title": "Left Sidebar Page",
			"postTypes": [ "page,post" ]
    },
    {
			"name": "page-right-sidebar",
			"title": "Right Sidebar Page",
			"postTypes": [ "page,post" ]
    },
    {
			"name": "404",
			"postTypes": [
				"page"
			],
			"title": "404"
	}
  ],
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"color": {
			"duotone": [
        {
          "colors": [
            "#EEEEEE",
            "#111"
          ],
          "name": "Bg prim & Prim Color",
          "slug": "bg-prim-and-prim-color"
        },
        {
          "colors": [
            "#FFFFFF",
            "#838181"
          ],
          "name": "Bg-sec and Secondary Color",
          "slug": "bg-sec-and-sec-color"
        },
        {
          "colors": [
            "#111",
            "#111"
          ],
          "name": "Accent & Prim-color",
          "slug": "accent-and-prim-color"
        },
        {
          "colors": [
            "#111",
            "#838181"
          ],
          "name": "Accent & Secondary Color",
          "slug": "accent-and-sec-color"
        }
      ],
			"gradients": [
				{
					"gradient": "linear-gradient(to bottom,var(--wp--preset--color--secondary) 0%,var(--wp--preset--color--tertiary) 100%)",
					"name": "Vertical secondary to tertiary",
					"slug": "vertical-secondary-to-tertiary"
				},
				{
					"gradient": "linear-gradient(to bottom,var(--wp--preset--color--secondary) 0%,var(--wp--preset--color--background) 100%)",
					"name": "Vertical secondary to background",
					"slug": "vertical-secondary-to-background"
				},
				{
					"gradient": "linear-gradient(to bottom,var(--wp--preset--color--tertiary) 0%,var(--wp--preset--color--background) 100%)",
					"name": "Vertical tertiary to background",
					"slug": "vertical-tertiary-to-background"
				},
				{
					"gradient": "linear-gradient(to bottom right,var(--wp--preset--color--primary) 0%,var(--wp--preset--color--light-text-color) 100%)",
					"name": "Diagonal primary to light-text-color",
					"slug": "diagonal-primary-to-light-text-color"
				},
				{
					"gradient": "linear-gradient(to bottom right,var(--wp--preset--color--secondary) 50%,var(--wp--preset--color--background) 50%)",
					"name": "Diagonal secondary to background",
					"slug": "diagonal-secondary-to-background"
				},
				{
					"gradient": "linear-gradient(to bottom right,var(--wp--preset--color--background) 50%,var(--wp--preset--color--secondary) 50%)",
					"name": "Diagonal background to secondary",
					"slug": "diagonal-background-to-secondary"
				},
				{
					"gradient": "linear-gradient(to bottom right,var(--wp--preset--color--tertiary) 50%,var(--wp--preset--color--background) 50%)",
					"name": "Diagonal tertiary to background",
					"slug": "diagonal-tertiary-to-background"
				},
				{
					"gradient": "linear-gradient(to bottom right,var(--wp--preset--color--background) 50%,var(--wp--preset--color--tertiary) 50%)",
					"name": "Diagonal background to tertiary",
					"slug": "diagonal-background-to-tertiary"
				}
			],
			 "palette": [
        {
               "color": "#FFF",
               "name": "Bg Primary",
               "slug": "bg-prim"
            },
            {
              "color": "#F4F5F6",
              "name": "Bg secondary",
              "slug": "bg-sec"
            },
            {
              "name": "Primary Color",
              "color": "#111",
              "slug": "prim-color"
            },
            {
              "name": "Secondary Color",
              "color": "#838181",
              "slug": "sec-color"
            },
            {
              "color": "#111",
              "name": "Accent",
              "slug": "accent"
            },
            {
              "color": "#FFF",
              "name": "Button Color",
              "slug": "btnclr"
            }
      ]
		},
		"custom": {
			"spacing": {
				"baseline": "16px",
				"gap": {
					"horizontal": "clamp( calc( 1.5 * var( --wp--custom--spacing--baseline ) ), 2.222vw, calc( 2 * var( --wp--custom--spacing--baseline ) ) )",
					"vertical": "clamp( calc( 1.5 * var( --wp--custom--spacing--baseline ) ), 3.333vw, calc( 3 * var( --wp--custom--spacing--baseline ) ) )"
				},
				"gutter": "clamp( calc( 1.5 * var( --wp--custom--spacing--baseline ) ), 3.333vw, calc( 3 * var( --wp--custom--spacing--baseline ) ) )",
				"outer": "var( --wp--custom--spacing--gutter )",
				"small": "min(24px, 6.4vw)"
			}
		},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1280px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"customFontSize": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Poppins",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/poppins/Poppins-Regular.ttf"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "Poppins",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/poppins/Poppins-Bold.ttf"
							]
						}
					],
					"fontFamily": "Poppins, serif",
					"name": "Poppins",
					"slug": "poppins"
				},
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Lato",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/Lato/Lato-Regular.ttf"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "Lato",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/Lato/Lato-Bold.ttf"
							]
						}
					],
					"fontFamily": "Lato, serif",
					"name": "Lato",
					"slug": "lato"
				},
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Inter",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "200 900",
							"src": [
								"file:./assets/fonts/Inter/Inter-VariableFont.ttf"
							]
						}
					],
					"fontFamily": "Inter, serif",
					"name": "Inter",
					"slug": "inter"
				},
						{
          "fontFace": [
			            {
			              "fontFamily": "Montserrat",
			              "fontStretch": "normal",
			              "fontStyle": "normal",
			              "fontWeight": "200 900",
			              "src": [
			                "file:./assets/fonts/Montserrat/Montserrat-VariableFont_wght.ttf"
			              ]
			            },
			            {
			              "fontFamily": "Montserrat",
			              "fontStretch": "normal",
			              "fontStyle": "italic",
			              "fontWeight": "400 900",
			              "src": [
			                "file:./assets/fonts/Montserrat/Montserrat-Italic-VariableFont_wght.ttf"
			              ]
			            }
          			],
				          "fontFamily": "\"Montserrat\", serif",
				          "name": "Montserrat",
				          "slug": "montserrat"
				},
					{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "OpenSans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/OpenSans/OpenSans-Regular.ttf"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "OpenSans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/OpenSans/OpenSans-Bold.ttf"
							]
						}
					],
					"fontFamily": "OpenSans, serif",
					"name": "OpenSans",
					"slug": "openSans"
				},
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Playfair_Display",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/Playfair_Display/Playfair_Display-VariableFont.ttf"
							]
						}
					],
					"fontFamily": "Playfair_Display, serif",
					"name": "Playfair_Display",
					"slug": "playfairdisplay"
				},
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Roboto",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/Roboto/Roboto-Regular.ttf"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "Roboto",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/Roboto/Roboto-Bold.ttf"
							]
						}
					],
					"fontFamily": "\"Roboto\", serif",
					"name": "Roboto",
					"slug": "roboto"
				},
					{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Vollkorn",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/Vollkorn/Vollkorn-VariableFont.ttf"
							]
						}
					],
					"fontFamily": "Vollkorn, serif",
					"name": "Vollkorn",
					"slug": "vollkorn"
				}
			],
			"fontSizes": [
				{
					"name": "Tiny",
					"size": "13px",
					"slug": "tiny"
				},
				{
					"name": "Extra Small",
					"size": "14px",
					"slug": "extra-small"
				},
				{
					"name": "Small",
					"size": "15px",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "16px",
					"slug": "medium"
				},
				{
					"name": "Upper Heading",
					"size": "18px",
					"slug": "upper-heading"
				},
				{
					"name": "Normal",
					"size": "clamp(18px,1.6vw,20px)",
					"slug": "normal"
				},
				{
					"name": "Content Heading",
					"size": "clamp(22px,4.2vw,24px)",
					"slug": "content-heading"
				},
				{
					"name": "Large",
					"size": "clamp(28px,4.2vw,32px)",
					"slug": "large"
				},
				{
					"name": "Extra large (Recent Post Title)",
					"size": "clamp(32px,6vw,36px)",
					"slug": "extra-large"
				},
				{
					"name": "Wide",
					"size": "clamp(27px,5vw,45px)",
					"slug": "wide"
				},
				{
					"name": "Vast",
					"size": "clamp(35px,5vw,55px)",
					"slug": "vast"
				},
				{
					"name": "Broad",
					"size": "clamp(36px,6vw,70px)",
					"slug": "broad"
				},
				{
					"name": "Huge (Display)",
					"size": "clamp(36px,5.2vw,80px)",
					"slug": "huge"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/columns": {
				"border": {
					"radius": "10px"
				  }
			},
			"core/navigation": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--upper-heading)",
					"fontWeight": "600",
					"lineHeight": "normal"
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/button": {
		        "border": {
		          "radius": "4px"
		        }
		   }
		},
		"color": {
			"background": "var(--wp--preset--color--bg-prim)",
			"text": "var(--wp--preset--color--prim-color)"
		},
		 "spacing": {
      "padding": {
				"right": "18px",
				"left": "18px"
			}
    },
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "normal"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "var(--wp--preset--font-size--upper-heading)",
					"lineHeight": "normal"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "var(--wp--preset--font-size--content-heading)",
					"lineHeight": "normal"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "var(--wp--preset--font-size--normal)",
					"lineHeight": "normal"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "normal"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "var(--wp--preset--font-size--extra-small)",
					"lineHeight": "normal"
				}
			},
			"link": {
				":active": {
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"typography": {
						"textDecoration": "none"
					}
				},
				":hover": {
					"typography": {
						"textDecoration": "none"
					}
				},
				"color": {
          		"text": "var(--wp--preset--color--prim-color)"
        },
        		"typography": {
          		"textDecoration": "none"
        }
				
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--poppins)",
			"fontSize": "var(--wp--preset--font-size--upper-heading)",
			"lineHeight": "normal"
		}
	}
}